<h1>Hello, <%= @current_user.username %></h1>
<p></br>
<h4>You have <%= @current_user.track_count %> tracks at SoundCloud:</h4>
<p><h4>Choose the one you want to connect to SoundDrop.</h4>
</br>




<%= form_for Story.new, :url => url_for(:controller => 'create', :action => 'upload_track') do |s| %>
    <% stories = @current_user_tracks.map do |track| %>
      <% {id: track.id}.each do |x,y| %>
        <br><%= radio_button_tag(:id, y) %>
        <%= label_tag(:id, track.title) %></br>
      <% end %>
    <% end %>

<% end %>

